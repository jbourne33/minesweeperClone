<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="359.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="31.0" prefWidth="154.0" BorderPane.alignment="CENTER">
         <children>
            <MenuButton fx:id="difficultyMenu" mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" text="Easy">
              <items>
                <MenuItem mnemonicParsing="false" text="Medium" />
                <MenuItem mnemonicParsing="false" text="Difficult" />
              </items>
            </MenuButton>
            <Button fx:id="startButton" mnemonicParsing="false" text="Start" />
            <AnchorPane prefHeight="31.0" prefWidth="288.0">
               <children>
                  <HBox alignment="CENTER_RIGHT" mouseTransparent="true" prefHeight="31.0" prefWidth="187.0">
                     <children>
                        <Label fx:id="bombStatus" alignment="CENTER_RIGHT" contentDisplay="RIGHT" nodeOrientation="RIGHT_TO_LEFT" text="BombStatus" textAlignment="RIGHT">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets right="2.0" />
                     </padding>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" mouseTransparent="true" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="messageBox" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="453.0" text="MessageBox" textAlignment="CENTER" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
